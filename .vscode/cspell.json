{
    "allowCompoundWords": true,
    "dictionaries": [
        "aws",
        "filetypes",
        "python"
    ],
    "ignorePaths": [
        "**/*.css",
        "**/*.js",
        "**/.git/",
        "**/__pycache__/**",
        "**/package-lock.json",
        "**/package.json",
        "**/poetry.lock",
        "**/src/dot_Brewfile.tmpl"

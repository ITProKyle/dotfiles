{
    "ignorePaths": [
        "**/*.css",
        "**/*.js",
        "**/*.pyc",
        "**/.venv/**",
        "**/__pycache__/**",
        "**/node_modules/**",
        "**/package-lock.json",
        "**/package.json",
        "**/poetry.lock",
        "**/tsconfig.json",
        ".vscode/dictionaries/*.txt",
        "typings/**"
    ],
    "language": "en",
    "languageSettings": [
        {
            "allowCompoundWords": true,
            "dictionaries": [
                "python"
            ],
            "languageId": "toml"
        }
    ],
    "maxNumberOfProblems": 100,
    "version": "0.2",
    "words": [
        "CPPFLAGS",
        "LDFLAGS",
        "XCRUN",
        "autofix",
        "bindkey",
        "catthehacker",
        "chezmoi",
        "direnv",
        "dotenv",
        "dotfiles",
        "envrc",
        "fgrep",
        "frontmatter",
        "iterm",
        "ltcl",
        "mdformat",
        "nvmrc",
        "pipenv",
        "pkgconfig",
        "pyenv",
        "readline",
        "shellcheck",
        "substr",
        "tcltk",
        "venv",
        "xcrun",
        "yamlfmt"
    ]
}
